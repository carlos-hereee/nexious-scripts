{
  "initContext": [
    "import { createContext, useCallback, useContext, useMemo, useReducer }  from 'react';",
    "import { I${name}Schema } from '${nameLower}-context';",
    "import { reducer,  A_${nameUpper}  } from './reducer';",
    "import { ChildProps } from 'app-types';",
    "import ${nameLower}State from '@data/state/${nameLower}State.json';",
    "import { LogContext } from '../log/LogContext';",
    "${requestImports}",
    "",
    "export const ${name}Context = createContext<I${name}Schema>({} as I${name}Schema );",
    "",
    "export const ${name}State =( {children}:ChildProps )=> {",
    "const [state, dispatch] = useReducer(reducer,  ${nameLower}State);",
    "const { setStatus } = useContext(LogContext);",
    "",
    "${contextDispatch}",
    "${contextRequest}",
    "",
    "const ${nameLower}Values = useMemo(()=>{",
    "return { ",
    "${contextValues}",
    " }; }, [  ${contextTriggers}  ]);",
    "",
    "return <${name}Context.Provider value={${nameLower}Values}> {children} </${name}Context.Provider>;",
    "};"
  ],
  "initReducer": [
    "import { ${name}ActionProps, P${name}State } from '${nameLower}-context';",
    "",
    "export enum A_${nameUpper} { ${reducerTypes} }",
    "export const reducer = (state: P${name}State, action: ${name}ActionProps): P${name}State => {",
    "  switch (action.type) { ",
    "${reducerCase}",
    "    default:",
    "return state;",
    "}};"
  ],
  "initRequest": [
    "import { A_${name} } from './reducer';import { axiosAuth } from '@axios/axiosAuth';import { isDev } from '@config';",
    "import { ${name}RequesthProps } from '${nameLower}-context';",
    "",
    "export const ${requestName} = async ({ values, dispatch, setStatus }: ${name}RequesthProps) => {",
    "try { setStatus('LOADING');",
    "// REQUEST DATA HERE",
    "setStatus('SUCCESS');",
    "} catch (error) {",
    "if (isDev) console.log('error', error);}};"
  ]
}
